---
import Layout from "../../layouts/Layout.astro";
import Navbar from "../../components/Navbar.astro";
import Main from "../../components/Main.astro";
import Footer from "../../components/Footer.astro";
import FormularioDeRastreo from "../../components/RastrearPaquete/FormularioDeRastreo.astro";
import PreguntasFrecuentes from "../../components/PreguntasFrecuentes.astro";

import { DiccionarioTitulos } from "../../translations/DiccionarioTitulos";
---

<Layout
  Idioma="es"
  Encabezado="RASTREAR_PAQUETE"
  Titulo={DiccionarioTitulos.tituloRastrearPaquete["es"]}
>
  <Navbar Idioma="es" />
  <Main>
    <FormularioDeRastreo Idioma="es" />
    <PreguntasFrecuentes Idioma="es" Preguntas="RASTREO" />
    <Footer Idioma="es" />
  </Main>
</Layout>
