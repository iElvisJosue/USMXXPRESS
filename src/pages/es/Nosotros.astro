---
import Layout from "../../layouts/Layout.astro";
import Navbar from "../../components/Navbar.astro";
import Main from "../../components/Main.astro";
import Footer from "../../components/Footer.astro";
import Presentacion from "../../components/Nosotros/Presentacion.astro";
import Filosofia from "../../components/Nosotros/Filosofia.astro";
import PreguntasFrecuentes from "../../components/PreguntasFrecuentes.astro";

import { DiccionarioTitulos } from "../../translations/DiccionarioTitulos";
---

<Layout Idioma="es" Titulo={DiccionarioTitulos.tituloNosotros["es"]}>
  <Navbar Idioma="es" />
  <Main>
    <Presentacion Idioma="es" />
    <Filosofia Idioma="es" />
    <PreguntasFrecuentes Idioma="es" Preguntas="NOSOTROS" />
