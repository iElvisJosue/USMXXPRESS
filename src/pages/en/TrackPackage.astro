---
import Layout from "../../layouts/Layout.astro";
import Navbar from "../../components/Navbar.astro";
import Main from "../../components/Main.astro";
import Footer from "../../components/Footer.astro";
import FormularioDeRastreo from "../../components/RastrearPaquete/FormularioDeRastreo.astro";
import PreguntasFrecuentes from "../../components/PreguntasFrecuentes.astro";

import { DiccionarioTitulos } from "../../translations/DiccionarioTitulos";
---

<Layout
  Idioma="en"
  Encabezado="RASTREAR_PAQUETE"
  Titulo={DiccionarioTitulos.tituloRastrearPaquete["en"]}
>
  <Navbar Idioma="en" />
  <Main>
    <FormularioDeRastreo Idioma="en" />
    <PreguntasFrecuentes Idioma="en" Preguntas="RASTREO" />
    <Footer Idioma="en" />
  </Main>
</Layout>
