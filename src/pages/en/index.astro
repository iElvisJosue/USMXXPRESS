---
import Layout from "../../layouts/Layout.astro";
import Navbar from "../../components/Navbar.astro";
import Main from "../../components/Main.astro";
import Principal from "../../components/index/Principal.astro";
import NuestrosServicios from "../../components/index/NuestrosServicios.astro";
import PorqueElegirnos from "../../components/index/PorqueElegirnos.astro";
import PreguntasFrecuentes from "../../components/PreguntasFrecuentes.astro";
import Footer from "../../components/Footer.astro";

import { DiccionarioTitulos } from "../../translations/DiccionarioTitulos";
---

<Layout Titulo={DiccionarioTitulos.tituloPrincipal["en"]} Idioma="en">
  <Navbar Idioma="en" />
  <Main>
    <Principal Idioma="en" />
    <NuestrosServicios Idioma="en" BgContenido={true} />
    <PorqueElegirnos Idioma="en" />
    <PreguntasFrecuentes Idioma="en" Preguntas="GENERALES" />
    <Footer Idioma="en" />
  </Main>
</Layout>
